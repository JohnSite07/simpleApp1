{
    "include": [
        "simpleApp1",
        "home",
        "weighttracker"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/migrations",
        "staticfiles"
    ],
    "ignore": [],
    "defineConstant": {
        "DEBUG": true
    },
    "typeCheckingMode": "basic",
    "useLibraryCodeForTypes": true,
    "autoImportCompletions": true,
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.12",
    "pythonPlatform": "Windows",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.12",
            "pythonPlatform": "Windows",
            "extraPaths": [
                "./simpleApp1",
                "./home", 
                "./weighttracker"
            ]
        }
    ]
} 